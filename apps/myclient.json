{
    "name": "My-Client API",
    "api_id": "2",
    "org_id": "default",
    "definition": {
        "location": "header",
        "key": "version"
    },
    "use_oauth2": true,
    "allowed_access_types":[
         "authorization_code",
         "refresh_token",
         "password",
         "client_credentials"
      ],
      "allowed_authorize_types":[
         "code",
         "token"
      ],
      "auth_login_redirect":"<auth_login_redirect>",
    "version_data": {
        "not_versioned": true,
        "versions": {
            "Default": {
                "name": "Default",
                "expires": "3000-01-02 15:04",
                "use_extended_paths": true,
                "extended_paths": {
                    "ignored": [],
                    "white_list": [],
                    "black_list": []
                }
            }
        }
    },
    "proxy": {
        "listen_path": "/myclient/",
        "target_url": "http://httpbin.org",
        "strip_listen_path": true
    },
    "enable_batch_request_support": true
}
